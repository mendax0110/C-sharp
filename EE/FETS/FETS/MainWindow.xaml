<Window x:Class="FETS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FETS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel Width="200" Background="Gray">
            <Label Content="Transistor:"></Label>
            <TextBox x:Name="TransistorName" Text=""></TextBox>
            <Label Content="TransistorVoltage"></Label>
            <TextBox x:Name="TransistorVoltage" Text="" TextChanged="TransistorVoltage_TextChanged"></TextBox>
            <Label Content="" Height="14"></Label>
            <Button x:Name="ButtonAdd" Content="Add" Click="ButtonAdd_Click"></Button>
            <Label Content="" Height="10"></Label>
            <Button x:Name="ButtonReplace" Content="Replace" Click="ButtonReplace_Click"></Button>
        </StackPanel>
        <ListBox x:Name="ListBoxOutput"></ListBox>
    </DockPanel>
</Window>
